package first;
import java.awt.*;
import javax.swing.*;
import java.applet.*;
public class Beginning extends Applet implements Runnable{
	public static final long serialVersionUID = 1L;
	
	public static int pixelSize = 3;
	
	public static Dimension size = new Dimension(800, 600);
	public static Dimension pixel = new Dimension(size.getWidth() / pixelSize, size.getHeight() / pixelSize);
	
	public Beginning() {
		
	}
	
	public void start() {
		
	}
	
	public void stop() {
		
	}
	
	public void run() {
		
	}
	
	public static void main(String args[]) {
		Beginning beginning = new Beginning();
		
		JFrame frame = new JFrame();
		frame.add(beginning);
		frame.pack();
	}
}
